<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="umd.mindlab.main" android:versionCode="1" android:versionName="1.0">
	<uses-sdk android:minSdkVersion="15" />

	<application android:icon="@drawable/locateme" android:label="@string/app_name">
		<activity android:name=".LocateMeActivity" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter> 
		</activity>
		<receiver android:name=".WifiReceiver">
			<intent-filter>
				<action android:name="umd.mindlab.objects" />
			</intent-filter>
		</receiver>

	</application>
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"></uses-permission>
	<uses-permission android:name="android.permission.CHANGE_WIFI_STATE"></uses-permission>
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


	<!-- Needed only if your app targets Android 5.0 (API level 21) or higher.
	<uses-feature android:name="android.hardware.location.gps" /> -->
</manifest>