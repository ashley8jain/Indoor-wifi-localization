<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="umd.mindlab.main"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--
    Needed only if your app targets Android 5.0 (API level 21) or higher.
	<uses-feature android:name="android.hardware.location.gps" />
    -->
    <application
        android:icon="@drawable/locateme"
        android:label="@string/app_name" >
        <activity
            android:name="umd.mindlab.main.LocateMeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="umd.mindlab.main.WifiReceiver" >
            <intent-filter>
                <action android:name="umd.mindlab.objects" />
            </intent-filter>
        </receiver>

        <provider
            android:name="com.android.tools.fd.runtime.InstantRunContentProvider"
            android:authorities="umd.mindlab.main.com.android.tools.fd.runtime.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>